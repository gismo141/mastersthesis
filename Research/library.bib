@article{Benzerrouk2013,
	Author = {Benzerrouk, Hamza and Salhi, Hassen and Nebylov, Alexander},
	Date-Added = {2015-08-21 14:59:34 +0000},
	Date-Modified = {2015-08-23 07:49:00 +0000},
